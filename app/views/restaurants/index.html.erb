<% if @restaurants.any? %>
  <% @restaurants.each do |restaurant| %>
    <h2><%= link_to "#{restaurant.name}", restaurant_path(restaurant) %><h2>
    <%= link_to "Edit #{restaurant.name}", edit_restaurant_path(restaurant) %>
    
  <% end %>
<% else %>
  No restaurants yet!
<% end %>

<a href="<%=new_restaurant_path%>">Add a restaurant</a>